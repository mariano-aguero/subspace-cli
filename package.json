{
  "name": "subspace-cli",
  "version": "0.1.2",
  "description": "subspace cli",
  "author": "Mariano Aguero @mariano-aguero",
  "bin": {
    "subspace-cli": "./bin/run"
  },
  "homepage": "https://github.com/mariano-aguero/subspace-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/mariano-aguero/subspace-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.20.4",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.7",
    "@subspace/subspace": "^0.2.0",
    "@types/file-type": "^10.9.1",
    "file-type": "^16.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.17",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.11.8",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "subspace-cli",
    "dirname": "subspace-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "storage": {
        "description": "Allows to create and submit a signed transaction with a file and return an objectId, also to return the object itself."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/mariano-aguero/subspace-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
